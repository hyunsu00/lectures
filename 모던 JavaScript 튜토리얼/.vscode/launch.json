{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "node.js Launch Program",
			"program": "${workspaceFolder}/${fileBasenameNoExtension}${fileExtname}",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:5500/${fileBasenameNoExtension}${fileExtname}",
			"webRoot": "${workspaceFolder}"
		}
	]
}